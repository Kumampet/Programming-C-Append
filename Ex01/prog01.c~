#include <stdio.h>

int main(){
  short s;
  int i;
  long l;

  float f;
  double d;

  printf("sizeof(short):%d\n",sizeof(s));
  printf("sizeof(int):%d\n",sizeof(i));
  printf("sizeof(long):%d\n",sizeof(l));
  printf("sizeof(float):%d\n",sizeof(f));
  printf("sizeof(double):%d\n",sizeof(d));

  return 0;
}
